<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form">

	<l:BlockLayout background="Light">
		<l:BlockLayoutRow>
			<l:BlockLayoutCell title="" />
			<l:BlockLayoutCell title="Shooter">
				<Button text="Shootertabelle bearbeiten" press="onMaintainShooterPress" visible="{authModel>/0/admin}"/>
				<Table items="{Shooter1>/}">
					<columns>
						<Column hAlign="Center">
							<Text text="Rank" />
						</Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
							<Text text="Nummer" />
						</Column>
						<Column hAlign="Center">
							<Text text="Nickname" />
						</Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
							<Text text="Punkte" />
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<ObjectIdentifier title="{Shooter1>rank}" />
								<ObjectNumber number="{Shooter1>gamer_number}" />
								<ObjectIdentifier title="{Shooter1>nickname}" />
								<ObjectNumber number="{Shooter1>points}" />
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</l:BlockLayoutCell>
			<l:BlockLayoutCell title="" />
		</l:BlockLayoutRow>
