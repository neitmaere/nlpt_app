								<ObjectIdentifier title="{Shooter1>rank}" />
								<ObjectNumber number="{Shooter1>gamer_number}" />
								<ObjectIdentifier title="{Shooter1>nickname}" />
								<ObjectNumber number="{Shooter1>points}" />
