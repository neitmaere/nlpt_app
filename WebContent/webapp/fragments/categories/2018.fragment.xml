				<Table items="{Shooter1>/}">
					<columns>
						<Column hAlign="Center">
							<Text text="Rank" />
						</Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
							<Text text="Nummer" />
						</Column>
						<Column hAlign="Center">
							<Text text="Nickname" />
						</Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
							<Text text="Punkte" />
						</Column>
					</columns>
					<items>
						<ColumnListItem>
